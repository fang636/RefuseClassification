.myContainer {
  border-top: 1rpx solid #e0e0e0;
  display: flex;
  flex-direction: column;
  text-align: center;
  padding-top: 30rpx;
}

.textBox {
  display: flex;
  flex-direction: column;
}

.textBox_warp {
  display: flex;
  border-bottom: 2rpx solid #e0e0e0;
  padding: 20rpx 0;
}

.textBox :last-child {
  border-bottom: none;
}

.textBox_warp_big {
  display: flex;
  border-bottom: 2rpx solid #e0e0e0;
  padding: 20rpx 0;
}

.warp_head {
  width: 200rpx;
}

.warp_body {
  flex: 1;
}

.fillBtn {
  width: 90%;
  margin-top: 60rpx;
}

.info {
  border: 1rpx solid #eee;
  background-color: #f8f8f8;
  padding: 20rpx;
  margin-bottom: 15rpx;
  box-shadow: 0 0 5rpx #e0e0e0;
  font-size: 34rpx;
}

.info .title {
  display: flex;
  color: #6a6a6a;
  margin-bottom: 20rpx;
}

.info .title .tit {
  width: 200rpx;
  text-align: left;
}

.info .title .state {
  flex: 1;
}

.info .content {
  border-top: 5rpx #ccc dotted;
  display: flex;
  flex-direction: column;

}

.info .content text {
  padding-top: 10rpx;
}
