.myContainer {
  display: flex;
  flex-direction: column;
  padding-top: 30rpx;
  margin: 0 20rpx;
  font-size: 40rpx;
}

.textBox {
  display: flex;
  flex-direction: column;
}

.textBox_warp {
  display: flex;
  border-bottom: 2rpx solid #e0e0e0;
  padding: 20rpx 0;
}

.textBox_warp_big {
  display: flex;
  border-bottom: 2rpx solid #e0e0e0;
  padding: 20rpx 0;
}

.warp_head {
  font-size: 30rpx;
  width: 200rpx;
  text-align: center;
}

.warp_body {
  flex: 1;
  font-weight: 600rpx;
}

.fillBtn {
  width: 90%;
  margin-top: 45rpx;
}

.picker {
  display: flex;

  align-items: center;
  margin-top: 5rpx;
  width: 100%;
  background-color: #f5f5f5;
  height: 80rpx;
  line-height: 80rpx;
}
.picker .picker_head{
  text-align: left;
}
.picker .picker_body{
  text-align: right;
}

